@prefix ns1: <http://www.opengis.net/ont/crs/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix void: <http://rdfs.org/ns/void#> .

ns1:TopographicMap a owl:Class,
        <http://www.w3.org/ns/ldp#Resource>,
        <https://www.iana.org/assignments/media-types/text/turtle#Resource> ;
    rdfs:label "topographic map"@en ;
    void:inDataset <http://www.opengis.net/geoepsg_dataset> ;
    ns1:uses ns1:ConformalProjection ;
    rdfs:subClassOf ns1:SRSApplication ;
    skos:definition "A type of map characterized by large-scale detail and quantitative representation of relief."@en .

