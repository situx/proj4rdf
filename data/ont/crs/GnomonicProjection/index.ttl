<http://www.opengis.net/ont/crs/GnomonicProjection> a owl:Class,
        <http://www.w3.org/ns/ldp#Resource>,
        <https://www.iana.org/assignments/media-types/text/turtle#Resource> ;
    rdfs:label "gnomonic projection"@en ;
    void:inDataset <http://www.opengis.net/geoepsg_dataset> ;
    rdfs:subClassOf <http://www.opengis.net/ont/crs/AzimuthalProjection> ;
    skos:definition "A map projection which displays all great circles as straight lines, resulting in any straight line segment on a gnomonic map showing a geodesic, the shortest route between the segment's two endpoints"@en ;
    foaf:image "https://situx.github.io/proj4rdf/projections/GnomonicProjection.svg"^^xsd:anyURI .

