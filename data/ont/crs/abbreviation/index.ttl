@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix void: <http://rdfs.org/ns/void#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://www.opengis.net/ont/crs/abbreviation> a owl:DatatypeProperty,
        <http://www.w3.org/ns/ldp#Resource>,
        <https://www.iana.org/assignments/media-types/text/turtle#Resource> ;
    rdfs:label "axis abbreviation"@en ;
    void:inDataset <http://www.opengis.net/geoepsg_dataset> ;
    rdfs:domain <http://www.opengis.net/ont/crs/CoordinateSystemAxis> ;
    rdfs:isDefinedBy <http://docs.opengeospatial.org/as/18-005r4/18-005r4.html> ;
    rdfs:range xsd:string ;
    skos:definition "The abbreviation used to identify an axis"@en .

