geocrs:SingleCRS a owl:Class ;
    rdfs:label "single coordinate reference system"@en ;
    rdfs:isDefinedBy <http://docs.opengeospatial.org/as/18-005r4/18-005r4.html> ;
    rdfs:subClassOf geocrs:CRS ;
