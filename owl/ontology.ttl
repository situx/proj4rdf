    rdfs:isDefinedBy <http://docs.opengeospatial.org/as/18-005r4/18-005r4.html> ;
    skos:definition "non-repeating sequence of coordinate system axes that spans a given coordinate space"@en .

geocrs:CRS a owl:Class ;
